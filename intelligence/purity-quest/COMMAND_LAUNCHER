# Function to execute commands
execute_command() {
    case $1 in
        1)
            echo "ğŸ¯ PURE_IP_FINDER - Enter domain for IP reconnaissance:"
            read -p "Domain: " domain
            if [[ -n "$domain" ]]; then
                find ip-"$domain"
            else
                echo "âŒ No domain provided!"
            fi
            ;;
        2)
            echo "ğŸ”’ Enabling anonymity mode..."
            make-me-anonymous
            ;;
        3)
            echo "ğŸ‘ï¸ Making you visible..."
            make-me-visible
            ;;
        4)
            echo "ğŸ•µï¸ Hiding your identity..."
            hide-me
            ;;
        5)
            echo "ğŸ”„ Toggling anonymity..."
            anonymous
            ;;
        6)
            echo "ğŸ” Checking anonymity status..."
            /usr/local/bin/make-me-anonymous status
            ;;
        7)
            echo "ğŸ­ Generating new identity..."
            /usr/local/bin/make-me-anonymous identity
            ;;
        8)
            echo "ğŸš¨ Emergency anonymity stop..."
            /usr/local/bin/make-me-anonymous emergency
            ;;
        9)
            echo "ğŸ”® Launching Purity Quest OSINT tool..."
            python3 ~/purity_quest.py
            ;;
        10)
            echo "ğŸ† Making you pure..."
            make-me-pure
            ;;
        11)
            echo "ğŸ”Œ Launching Pure USB toolkit..."
            python3 ~/pure_usb.py
            ;;
        12)
            echo "ğŸ“¸ Launching Pure Pics steganography..."
            python3 ~/Pure_Pics.py
            ;;
        13)
            echo "ğŸ’¥ Launching Pure Payloads generator..."
            python3 ~/Desktop/Pure_Payloads.py
            ;;
        14)
            echo "ğŸ“¨ Launching SMS Sender GUI..."
            python3 ~/sms_sender_gui.py
            ;;
        15)
            echo "ğŸ“± Launching Android SMS Sender..."
            python3 ~/android_sms_sender.py
            ;;
        16)
            echo "ğŸ“§ Launching Email SMS Sender..."
            python3 ~/email_sms_sender.py
            ;;
        17)
            echo "ğŸŸ¢ Starting SMS service..."
            ./start_sms_service.sh
            ;;
        18)
            echo "ğŸ”´ Stopping SMS service..."
            ./stop_sms_service.sh
            ;;
        19)
            echo "ğŸ“Š Checking SMS service status..."
            ./check_sms_service.sh
            ;;
        20)
            echo "ğŸ•µï¸ Sending anonymous SMS..."
            ./send_anonymous_sms.sh
            ;;
        21)
            echo "ğŸŒ Launching theHarvester GUI..."
            python3 ~/theHarvester_gui.py
            ;;
        22)
            echo "ğŸ“ Launching PhoneInfoga..."
            read -p "Enter phone number: " phone
            if [[ -n "$phone" ]]; then
                phoneinfoga scan -n "$phone"
            else
                phoneinfoga
            fi
            ;;
        23)
            echo "ğŸ” Launching Mosint..."
            read -p "Enter email address: " email
            if [[ -n "$email" ]]; then
                mosint "$email"
            else
                mosint
            fi
            ;;
        24)
            echo "ğŸ” Web Technology Scanner..."
            read -p "Enter URL/domain: " url
            if [[ -n "$url" ]]; then
                webtech "$url"
            else
                echo "âŒ No URL provided!"
            fi
            ;;
        25)
            echo "ğŸŒ Bing IP-to-Hosts discovery..."
            read -p "Enter IP address: " ip
            if [[ -n "$ip" ]]; then
                bing-ip2hosts "$ip"
            else
                echo "âŒ No IP provided!"
            fi
            ;;
        26)
            echo "ğŸ§  Pure Face - Reverse image OSINT..."
            read -p "Enter image path: " path
            if [[ -n "$path" ]]; then
                if [[ -x "/home/xx/purity-quest/.venv/bin/python" ]]; then
                    "/home/xx/purity-quest/.venv/bin/python" /home/xx/purity-quest/PURE_FACE "$path"
                else
                    python3 /home/xx/purity-quest/PURE_FACE "$path"
                fi
            else
                echo "âŒ No image path provided!"
            fi
            ;;
        27)
            echo "ğŸ”§ Google Search..."
            read -p "Enter search query: " query
            if [[ -n "$query" ]]; then
                google "$query"
            else
                echo "âŒ No search query provided!"
            fi
            ;;
        28)
            echo "ğŸ§ Launching Katoolin..."
            katoolin
            ;;
        29)
            echo "ğŸŒ Launching Ngrok tunnel..."
            read -p "Enter port to tunnel (default: 8080): " port
            port=${port:-8080}
            ngrok http "$port"
            ;;
        30)
            echo "ğŸ¤– Ollama AI Model Management..."
            echo "Available commands: list, pull, run, rm"
            read -p "Enter ollama command: " cmd
            if [[ -n "$cmd" ]]; then
                ollama $cmd
            else
                ollama list
            fi
            ;;
        31)
            echo "ğŸš€ Run Command..."
            run
            ;;
        32)
            echo "ğŸ‘¤ Face Detection..."
            read -p "Enter image/video path: " path
            if [[ -n "$path" ]]; then
                face_detection "$path"
            else
                echo "âŒ No file path provided!"
            fi
            ;;
        33)
            echo "ğŸ†” Face Recognition..."
            read -p "Enter image path: " path
            if [[ -n "$path" ]]; then
                face_recognition "$path"
            else
                echo "âŒ No image path provided!"
            fi
            ;;
        34)
            echo "ğŸ“š Opening full commands documentation..."
            less "/home/xx/Desktop/my custom commands.txt"
            ;;
        35)
            echo "ğŸ“‹ Viewing simple commands reference..."
            cat ~/command
            ;;
        36)
            echo "âœï¸ Opening commands editor..."
            echo "Which file to edit?"
            echo "1) Simple commands (~/command)"
            echo "2) Full documentation"
            read -p "Choice (1-2): " edit_choice
            case $edit_choice in
                1) nano ~/command ;;
                2) nano "/home/xx/Desktop/my custom commands.txt" ;;
                *) echo "âŒ Invalid choice!" ;;
            esac
            ;;
        q|Q|quit|exit)
            echo "ğŸ‘‹ Goodbye! Stay pure!"
            exit 0
            ;;
        *)
            echo "âŒ Invalid selection! Please choose 1-35 or 'q' to quit."
            return 1
            ;;
    esac
}

# Main loop
while true; do
    echo ""
    echo "ğŸ¯ Select a command (1-36) or 'q' to quit:"
    read -p "Your choice: " choice
    
    if [[ "$choice" == "q" || "$choice" == "Q" || "$choice" == "quit" || "$choice" == "exit" ]]; then
        echo "ğŸ‘‹ Goodbye! Stay pure!"
        break
    fi
    
    execute_command "$choice"
    
    echo ""
    echo "Press Enter to continue or 'q' to quit..."
    read -r continue_choice
    if [[ "$continue_choice" == "q" || "$continue_choice" == "Q" ]]; then
        echo "ğŸ‘‹ Goodbye! Stay pure!"
